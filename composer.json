{
  "name": "bytic/google-analytics",
  "type": "library",
  "description": "Google Analytics interaction library",
  "keywords": [
    "google analytics"
  ],
  "homepage": "https://github.com/ByTIC/services-google-analytics",
  "license": "MIT",
  "authors": [
    {
      "name": "Gabriel Solomon",
      "email": "hello@gabrielsolomon.ro"
    }
  ],
  "autoload": {
    "psr-4": {
      "ByTIC\\GoogleAnalytics\\Tracking\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ByTIC\\GoogleAnalytics\\Tests\\": "tests/src",
      "ByTIC\\GoogleAnalytics\\Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "require": {
    "php": ">=7.0",
    "ext-json": "*"
  },
  "require-dev": {
    "bytic/phpqatools": "^1.0"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "ergebnis/composer-normalize": true,
      "dealerdirect/phpcodesniffer-composer-installer": true,
      "bytic/phpqatools": true
    }
  }
}
